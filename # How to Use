# How to Use

matrix_size = 1000  # Adjust the size based on your problem
matrix = np.zeros((matrix_size, matrix_size), dtype=np.int32)

# Run and measure parallel implementation
parallel_time = measure_execution_time(lambda: run_parallel_swamp(matrix))

# Run and measure sequential implementation
sequential_time = measure_execution_time(lambda: swamp_sequential(matrix))

print("Sequential Execution Time: {} ms".format(sequential_time))
print("Speedup: {:.2f}x".format(sequential_time / parallel_time))
